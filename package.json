{
	"name": "ai-clicker",
	"description": "A Cloudflare Worker project.",
	"license": "MIT",
	"private": true,
	"engines": {
		"node": ">=20"
	},
	"type": "module",
	"scripts": {
		"dev": "bun run ./scripts/dev.ts",
		"build": "react-router build",
		"deploy": "wrangler deploy",
		"start": "wrangler dev --test-scheduled",
		"typecheck": "tsc",
		"rr:typegen": "react-router typegen",
		"rr:routes": "react-router routes",
		"quality": "biome check .",
		"quality:fix": "biome check . --write --unsafe",
		"predb:migrate:local": "bun run orm:generate",
		"db:drop": "rm -rf ./.wrangler",
		"db:seed": "wrangler d1 execute --local --file=./db/seed.sql",
		"db:migrate:local": "bun run db:migrate --local",
		"postdb:migrate:local": "bun run quality:fix",
		"db:migrate": "wrangler d1 migrations apply",
		"orm:generate": "drizzle-kit generate --config drizzle.config.ts",
		"setup": "bun run ./scripts/setup.ts"
	},
	"dependencies": {
		"@edgefirst-dev/api-client": "^0.1.0",
		"@edgefirst-dev/data": "^0.0.4",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@paralleldrive/cuid2": "^2.2.2",
		"@react-router/cloudflare": "^7.2.0",
		"@react-router/fs-routes": "^7.2.0",
		"bcryptjs": "^3.0.2",
		"bowser": "^2.11.0",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.40.0",
		"edgekitjs": "^0.0.47",
		"inflected": "^2.1.0",
		"isbot": "^5.1.23",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.2.0",
		"remix-auth": "^4.1.0",
		"tailwind-merge": "^3.0.2",
		"urlpattern-polyfill": "^10.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@cloudflare/workers-types": "^4.20250224.0",
		"@octokit/core": "^6.1.4",
		"@react-router/dev": "^7.2.0",
		"@tailwindcss/vite": "^4.0.9",
		"@total-typescript/ts-reset": "^0.6.1",
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/bun": "^1.2.4",
		"@types/inflected": "^2.1.3",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.7",
		"drizzle-kit": "^0.30.5",
		"msw": "^2.7.3",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.0.9",
		"typescript": "^5.8.2",
		"vite": "^6.2.0",
		"vite-env-only": "^3.0.3",
		"vite-plugin-cjs-interop": "^2.1.6",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^3.111.0"
	},
	"trustedDependencies": ["@biomejs/biome"],
	"repository": {
		"type": "git",
		"url": "https://github.com/sergiodxa/ai-clicker"
	}
}
